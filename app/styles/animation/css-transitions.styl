
/*******************************
     svet-corporate animation
********************************/

.svet-corporate.ng-enter,
.svet-corporate.ng-hide-add,
.svet-corporate.ng7hide-remove,
.svet-corporate.ng-leave
  transition(1s) !important;

.svet-corporate.ng-enter,
.svet-corporate.ng-hide-remove,
.svet-corporate.ng-hide-add.ng-hide-add-active,
.svet-corporate.ng-leave.ng-leave-active
  opacity 0 !important

.svet-corporate.ng-enter-stagger
   transition-delay: 1s; !important
   transition-duration: 0s; !important


.svet-corporate.ng-enter.ng-enter-active,
.svet-corporate.ng-hide-add,
.svet-corporate.ng-hide-remove.ng-hide-remove-active,
.svet-corporate.ng-leave
  opacity 1 !important

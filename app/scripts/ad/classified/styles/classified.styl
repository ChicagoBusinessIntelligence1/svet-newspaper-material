@import 'nib'

.tabs-classified
  overflow hidden
  min-height 200px

.cl-height
  height 200px

[sv-animate-on-change]
  position relative
  top 0px
  opacity 1
  transition: all 1s;
  -webkit-transition: all 1s;

[sv-animate-on-change].changed
  top -200px
  opacity 0
  position relative
  transition: all 1s;
  -webkit-transition: all 1s;



